{
  "React Component with Framer Motion": {
    "prefix": "rmc",
    "body": [
      "import { motion } from 'framer-motion';",
      "",
      "interface ${1:ComponentName}Props {",
      "  ${2:prop}: ${3:string};",
      "}",
      "",
      "const ${1:ComponentName} = ({ ${2:prop} }: ${1:ComponentName}Props) => {",
      "  return (",
      "    <motion.div",
      "      initial={{ opacity: 0, y: 20 }}",
      "      animate={{ opacity: 1, y: 0 }}",
      "      transition={{ duration: 0.5 }}",
      "      className=\"${4:className}\"",
      "    >",
      "      ${5:// Component content}",
      "    </motion.div>",
      "  );",
      "};",
      "",
      "export default ${1:ComponentName};"
    ],
    "description": "Creates a React component with Framer Motion animation"
  },
  "Luxury Page Component": {
    "prefix": "luxpage",
    "body": [
      "import { motion } from 'framer-motion';",
      "",
      "const ${1:PageName} = () => {",
      "  return (",
      "    <div className=\"min-h-screen bg-luxury-light\">",
      "      {/* Hero Section */}",
      "      <motion.section",
      "        className=\"relative h-screen flex items-center justify-center\"",
      "        initial={{ opacity: 0 }}",
      "        animate={{ opacity: 1 }}",
      "        transition={{ duration: 1 }}",
      "      >",
      "        <div className=\"text-center space-y-8\">",
      "          <motion.h1",
      "            className=\"text-6xl md:text-8xl font-light tracking-luxury text-luxury-black\"",
      "            style={{ fontFamily: 'var(--font-logo)' }}",
      "            initial={{ opacity: 0, y: 50 }}",
      "            animate={{ opacity: 1, y: 0 }}",
      "            transition={{ delay: 0.5, duration: 0.8 }}",
      "          >",
      "            ${2:PAGE TITLE}",
      "          </motion.h1>",
      "          <motion.div",
      "            className=\"w-24 h-px bg-luxury-gold mx-auto\"",
      "            initial={{ width: 0 }}",
      "            animate={{ width: 96 }}",
      "            transition={{ delay: 1, duration: 0.8 }}",
      "          />",
      "        </div>",
      "      </motion.section>",
      "",
      "      {/* Content Section */}",
      "      <section className=\"py-24 px-4 max-w-6xl mx-auto\">",
      "        ${3:// Page content}",
      "      </section>",
      "    </div>",
      "  );",
      "};",
      "",
      "export default ${1:PageName};"
    ],
    "description": "Creates a luxury page component with hero section"
  },
  "Collection Card Component": {
    "prefix": "collectioncard",
    "body": [
      "import { motion } from 'framer-motion';",
      "import { Link } from 'wouter';",
      "",
      "interface CollectionCardProps {",
      "  title: string;",
      "  subtitle: string;",
      "  image: string;",
      "  href: string;",
      "  index?: number;",
      "}",
      "",
      "const CollectionCard = ({ title, subtitle, image, href, index = 0 }: CollectionCardProps) => {",
      "  return (",
      "    <motion.div",
      "      className=\"group relative overflow-hidden bg-white rounded-lg shadow-luxury hover:shadow-luxury-hover transition-all duration-500\"",
      "      initial={{ opacity: 0, y: 50 }}",
      "      animate={{ opacity: 1, y: 0 }}",
      "      transition={{ delay: index * 0.2, duration: 0.6 }}",
      "      whileHover={{ y: -10 }}",
      "    >",
      "      <Link href={href}>",
      "        <div className=\"aspect-[4/5] overflow-hidden\">",
      "          <motion.img",
      "            src={image}",
      "            alt={title}",
      "            className=\"w-full h-full object-cover transition-transform duration-700 group-hover:scale-110\"",
      "            loading=\"lazy\"",
      "          />",
      "        </div>",
      "        <div className=\"p-8 text-center\">",
      "          <h3 className=\"text-2xl font-light tracking-luxury text-luxury-black mb-2\">",
      "            {title}",
      "          </h3>",
      "          <p className=\"text-luxury-gold text-sm tracking-wide\">",
      "            {subtitle}",
      "          </p>",
      "        </div>",
      "      </Link>",
      "    </motion.div>",
      "  );",
      "};",
      "",
      "export default CollectionCard;"
    ],
    "description": "Creates a luxury collection card component"
  }
}